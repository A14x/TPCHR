--Properties:

ScreenGui.Parent = game.CoreGui

Main.Name = "Main"
Main.Parent = ScreenGui
Main.BackgroundColor3 = Color3.fromRGB(21, 21, 21)
Main.Position = UDim2.new(0.589235723, 0, 0.28407225, 0)
Main.Size = UDim2.new(0, 387, 0, 436)
Main.Active = true
Main.Draggable = true

Label.Name = "Label"
Label.Parent = Main
Label.BackgroundColor3 = Color3.fromRGB(126, 123, 126)
Label.Size = UDim2.new(0, 387, 0, 50)
Label.Font = Enum.Font.GothamBold
Label.Text = "Chicago Remastered TP GUI  | made by SwordWorthy"
Label.TextColor3 = Color3.fromRGB(0, 0, 0)
