TPgunStore.Name = "TPgunStore"
TPgunStore.Parent = Main
TPgunStore.BackgroundColor3 = Color3.fromRGB(14, 255, 50)
TPgunStore.BorderColor3 = Color3.fromRGB(53, 83, 104)
TPgunStore.BorderSizePixel = 2
TPgunStore.Position = UDim2.new(0.0197485797, 0, 0.137213469, 0)
TPgunStore.Size = UDim2.new(0, 183, 0, 52)
TPgunStore.Style = Enum.ButtonStyle.RobloxRoundButton
TPgunStore.Font = Enum.Font.SourceSans
TPgunStore.Text = "TP GUN STORE (SAFEZONE)"
TPgunStore.TextColor3 = Color3.fromRGB(0, 0, 0)
TPgunStore.TextSize = 18.000
TPgunStore.MouseButton1Down:connect(function()
	local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
	hrp.CFrame = CFrame.new(107.9, 4.1, 55.9)
end)

